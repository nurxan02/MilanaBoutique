@model Contact
@{
    ViewData["Title"] = "Index";
}


<div class="container">

    <table class="mt-3 table table-dark">
        <thead>
            <tr>
                <th>
                    #
                </th>
                <th>
                    Adress
                </th>

                <th>
                    Settings
                </th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>
                    @Model.Id
                </td>
                <td>
                    @Model.OfficeLocation
                </td>


                <td>
                    <a asp-controller="contact" asp-action="edit" asp-route-id="@Model.Id" class="btn btn-warning btn-icon-text">
                        <i class="mdi mdi-file-check btn-icon-append"></i>
                        Edit
                    </a>

                </td>
            </tr>
        </tbody>
    </table>
